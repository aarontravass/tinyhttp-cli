{
  "name": "@tinyhttp/cli",
  "description": "CLI to setup tinyhttp apps",
  "version": "1.3.4",
  "type": "module",
  "homepage": "https://tinyhttp.v1rtl.site",
  "funding": {
    "type": "individual",
    "url": "https://github.com/talentlessguy/tinyhttp?sponsor=1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tinyhttp/cli.git"
  },
  "bin": {
    "tinyhttp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12.4"
  },
  "keywords": [
    "tinyhttp",
    "router",
    "backend",
    "http",
    "framework",
    "api"
  ],
  "author": "v1rtl",
  "license": "MIT",
  "dependencies": {
    "cac": "^6.7.2",
    "colorette": "^1.2.2",
    "edit-json-file": "^1.6.0",
    "httpie": "^1.1.2",
    "ora": "^5.4.0",
    "which-pm-runs": "^1.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "node --experimental-loader esbuild-node-loader node_modules/uvu/bin.js",
    "test:coverage": "c8 --include=src pnpm test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "lint": "eslint . --ext=ts",
    "format": "prettier --check \"./**/*.ts\""
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/edit-json-file": "^1.4.0",
    "@types/which-pm-runs": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "earljs": "^0.1.10",
    "esbuild-node-loader": "^0.1.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.1",
    "rollup": "^2.52.1",
    "typescript": "^4.3.4",
    "uvu": "^0.5.1"
  }
}
